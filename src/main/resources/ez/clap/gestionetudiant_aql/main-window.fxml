<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<TabPane maxHeight="400.0" maxWidth="600.0" minHeight="400.0" minWidth="600.0" prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ez.clap.gestionetudiant_aql.controllers.MainWindowController">
  <tabs>
    <Tab text="Étudiant">
      <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
               <children>
                  <TableView fx:id="tableViewStudent" layoutX="176.0" layoutY="6.0" prefHeight="355.0" prefWidth="410.0">
                    <columns>
                      <TableColumn fx:id="tableColumnNumber" editable="false" prefWidth="100.79998779296875" resizable="false" text="Numéro" reorderable="false"/>
                      <TableColumn fx:id="tableColumnSecondName" editable="false" prefWidth="116.0" text="Nom" reorderable="false"/>
                        <TableColumn fx:id="tableColumnFirstName" editable="false" prefWidth="101.60003662109375" text="Prenom" reorderable="false"/>
                        <TableColumn fx:id="tableColumnCourse" editable="false" prefWidth="107.20001220703125" text="Cours" reorderable="false"/>
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
                  <VBox layoutX="6.0" layoutY="7.0" prefHeight="200.0" prefWidth="165.0">
                     <children>
                        <Button fx:id="buttonAddStudent" mnemonicParsing="false" onAction="#onButtonAddStudentClick" prefHeight="24.0" prefWidth="165.0" text="Ajouter un étudiant">
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin>
                        </Button>
                        <Button fx:id="buttonEditStudent" mnemonicParsing="false" onAction="#onButtonEditStudentClick" prefHeight="24.0" prefWidth="190.0" text="Modifier étudiant">
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin>
                        </Button>
                        <Button fx:id="buttonDeleteStudent" mnemonicParsing="false" onAction="#onButtonDeleteStudentClick" prefHeight="24.0" prefWidth="209.0" text="Supprimer étudiant">
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
               </children></AnchorPane>
      </content>
    </Tab>
    <Tab text="Cours">
      <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
               <children>
                  <TableView fx:id="tableViewCourse" layoutX="176.0" layoutY="6.0" prefHeight="355.0" prefWidth="410.0">
                    <columns>
                        <TableColumn prefWidth="120.00006103515625" text="Titre" reorderable="false"/>
                      <TableColumn prefWidth="143.199951171875" text="Numéro" reorderable="false"/>
                      <TableColumn prefWidth="148.800048828125" text="Code" reorderable="false"/>
                    </columns>
                  </TableView>
                  <VBox layoutX="6.0" layoutY="7.0" prefHeight="97.0" prefWidth="166.0">
                     <children>
                        <Button fx:id="buttonAddCourse" mnemonicParsing="false" prefHeight="24.0" prefWidth="172.0" text="Ajouter un cours">
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin>
                        </Button>
                        <Button fx:id="buttonEditCourse" mnemonicParsing="false" prefHeight="24.0" prefWidth="200.0" text="Modifier cours">
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin>
                        </Button>
                        <Button fx:id="buttonDeleteCourse" mnemonicParsing="false" prefHeight="24.0" prefWidth="184.0" text="Supprimer cours">
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
               </children></AnchorPane>
      </content>
    </Tab>
  </tabs>
</TabPane>
