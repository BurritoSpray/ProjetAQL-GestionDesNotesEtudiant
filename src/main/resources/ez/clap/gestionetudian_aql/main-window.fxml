<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="288.0" prefWidth="602.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ez.clap.gestionetudian_aql.controllers.MainWindowController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <GridPane prefHeight="193.0" prefWidth="591.0">
     <columnConstraints>
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
     </columnConstraints>
     <rowConstraints>
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
     </rowConstraints>
      <children>
      
          <Label fx:id="welcomeText" prefHeight="16.0" prefWidth="72.0" />
          <Button onAction="#onHelloButtonClick" text="Hello!" GridPane.columnIndex="1" />
         <ListView fx:id="listView" prefHeight="30.0" prefWidth="313.0" GridPane.columnSpan="2" GridPane.rowIndex="2" />
         <AnchorPane prefHeight="200.0" prefWidth="200.0">
            <children>
               <Button fx:id="myButton" layoutX="14.0" layoutY="11.0" mnemonicParsing="false" onAction="#onMyFuncClick" text="MyFunc" />
            </children>
         </AnchorPane>
      </children>
   </GridPane>
</VBox>
